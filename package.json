{
	"name": "delta-controls",
	"version": "2.0.2",
	"description": "A 3D movement controller.",
	"homepage": "https://github.com/vanruesc/delta-controls",
	"main": "build/bundle.js",
	"module": "src/index.js",
	"sideEffects": false,
	"license": "Zlib",

	"keywords": [
		"movement",
		"translation",
		"rotation",
		"object",
